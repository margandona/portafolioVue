# Frontend Environment Configuration for Payment Gateways
# Copy this file to .env.local and fill in your actual values

# Stripe Public Key (safe to expose in frontend)
VUE_APP_STRIPE_PUBLIC_KEY=pk_test_your_stripe_public_key_here

# API Base URL
VUE_APP_API_BASE_URL=http://localhost:3000/api

# Payment Configuration
VUE_APP_DEFAULT_CURRENCY=CLP
VUE_APP_PAYMENT_TIMEOUT=300000

# For production, use:
# VUE_APP_STRIPE_PUBLIC_KEY=pk_live_your_production_public_key
# VUE_APP_API_BASE_URL=https://your-production-api.com/api
